<?xml version="1.0" encoding="UTF-8"?>
<!-- MIS default -->
<configuration>
	<system.webServer>
    <security> 
        <ipSecurity allowUnlisted="true"> 
            <clear/> 
             <add ipAddress="127.0.0.1" allowed="true"/>
             <add ipAddress="::1" allowed="true"/>
             <add ipAddress="192.168.0.1" subnetMask="255.255.240.0" allowed="true" />
             <add ipAddress="10.8.0.0" subnetMask="255.255.255.0" allowed="true" />
             <add ipAddress="10.8.8.0" subnetMask="255.255.255.0" allowed="true" />
        </ipSecurity>  
    </security>
</system.webServer> 
</configuration>